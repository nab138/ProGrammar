{
  "name": "programmar",
  "private": true,
  "version": "1.0.2",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test.e2e": "cypress run",
    "test.unit": "vitest",
    "lint": "eslint",
    "wv": "node -e \"let version = require('./src/version.json'); let parts = version.version.split('.'); parts[2] = parseInt(parts[2]) + 1; version.version = parts.join('.'); require('fs').writeFileSync('./src/version.json', JSON.stringify(version, null, 2));\"",
    "nv": "npm version patch --no-git-tag-version && node -e \"let fs = require('fs'); let buildNumber = parseInt(fs.readFileSync('./build-number.txt', 'utf8')) + 1; fs.writeFileSync('./build-number.txt', buildNumber.toString());\" && npx capacitor-set-version set:ios -v $(node -p \"require('./package.json').version\") -b $(node -p \"require('fs').readFileSync('./build-number.txt', 'utf8')\") ."
  },
  "dependencies": {
    "@capacitor/app": "5.0.6",
    "@capacitor/core": "5.5.1",
    "@capacitor/haptics": "5.0.6",
    "@capacitor/ios": "5.5.1",
    "@capacitor/keyboard": "5.0.6",
    "@capacitor/live-updates": "^0.1.1-beta.4",
    "@capacitor/status-bar": "5.0.6",
    "@codemirror/lang-java": "^6.0.1",
    "@codemirror/lang-javascript": "^6.2.1",
    "@ionic/react": "^7.0.0",
    "@ionic/react-router": "^7.0.0",
    "@ionic/storage": "^4.0.0",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@uiw/codemirror-theme-okaidia": "^4.21.21",
    "@uiw/react-codemirror": "^4.21.21",
    "highlight.js": "^11.9.0",
    "ionicons": "^7.0.0",
    "markdown-to-jsx": "^7.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "react-shadow": "^20.4.0",
    "sonner": "^1.2.4"
  },
  "devDependencies": {
    "@capacitor/cli": "5.5.1",
